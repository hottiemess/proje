\newcommand{\internGlsShort}[1]{%
    \ifthenelse{\equal{\glsentrytype{#1}}{acronym}}{%
        \acrshort{#1}%
    }%
    {%
        \glstext{#1}%
    }%
}%
\newcommand{\myglsgen}[1]{%
    \glsdoifexists{#1}%
    {%
        \ifglsused{#1}{%
            \internGlsShort{#1}%
        }%
        {%
            \glsuseri{#1}~(\internGlsShort{#1})%
            \glsunset{#1}%
        }%
    }%
}%
